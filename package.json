{
  "name": "skybook",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "prettier": "prettier --write ./",
    "typescheck": "tsc --noEmit",
    "db-studio:dev": "drizzle-kit studio --port 3333",
    "db-studio:prod": "NODE_ENV=production drizzle-kit studio --port 3333",
    "db-push": "drizzle-kit push:sqlite",
    "db-generate": "NODE_ENV=production drizzle-kit generate:sqlite --out ./drizzle/migrations --breakpoints --schema=./src/server/db/schema.ts",
    "db-migrate": "NODE_ENV=production tsx drizzle/migrate",
    "db:dev": "turso dev --db-file local-db",
    "db-populate:dev": "tsx src/dataset",
    "db-populate:prod": "bun db-generate && bun db-migrate && NODE_ENV=production tsx src/dataset",
    "db-token": "turso db tokens create skybook",
    "turso-shell": "turso db shell skybook"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^0.3.6",
    "@fontsource/miriam-libre": "^5.0.18",
    "@fontsource/open-sauce-one": "^5.0.7",
    "@libsql/client": "^0.4.0-pre.5",
    "@t3-oss/env-nextjs": "^0.7.1",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.43.6",
    "@trpc/next": "^10.43.6",
    "@trpc/react-query": "^10.43.6",
    "@trpc/server": "^10.43.6",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "drizzle-orm": "^0.28.6",
    "lucide-react": "^0.309.0",
    "next": "^14.0.3",
    "next-auth": "^4.24.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "server-only": "^0.0.1",
    "sharp": "^0.33.1",
    "superjson": "^2.2.1",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.0.3",
    "@types/chalk": "^2.2.0",
    "@types/dotenv": "^8.2.0",
    "@types/eslint": "^8.44.7",
    "@types/node": "^18.17.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "autoprefixer": "^10.4.14",
    "better-sqlite3": "^9.2.2",
    "chalk": "^5.3.0",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.19.13",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "tailwindcss": "^3.3.5",
    "tsx": "^4.7.0",
    "typescript": "^5.1.6"
  },
  "ct3aMetadata": {
    "initVersion": "7.24.2"
  },
  "packageManager": "^bun@1.0.21"
}
